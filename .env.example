# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# AI Provider Configuration
# Anthropic Claude Proxy URL (required for Claude provider)
# In production, this should point to your deployed proxy server
# In development, use http://localhost:3001/anthropic-proxy
REACT_APP_ANTHROPIC_PROXY_URL=http://localhost:3001/anthropic-proxy

# Note: API keys for AI providers (Gemini, OpenAI, Claude) are configured
# through the application's Settings UI and stored in the database.
# They should NOT be stored in environment variables.
